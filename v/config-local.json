{
  "log": {
    "access": "${v2ray}/access.log",
    "error": "${v2ray}/error.log"
  },
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": [
          "relay"
        ],
        "outboundTag": "relay"
      }
    ]
  },
  "inbounds": [
    {
      "port": "${local-ss-port}",
      "protocol": "shadowsocks",
      "settings": {
        "email": "local@v2ray.com",
        "method": "aes-128-cfb",
        "password": "1"
      }
    },
    {
      "tag": "relay",
      "port": "${local-ss-relay-port}",
      "protocol": "shadowsocks",
      "settings": {
        "email": "local-relay@v2ray.com",
        "method": "aes-128-cfb",
        "password": "1"
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "${server-address}",
            "port": "${server-port}",
            "users": [
              {
                "id": "${server-password}",
                "alterId": 16
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "allowInsecure": true
        }
      },
      "mux": {
        "enabled": true,
        "concurrency": 8
      }
    },
    {
      "tag": "relay",
      "protocol": "socks",
      "settings": {
        "servers": [
          {
            "address": "${relay-server-address}",
            "port": "${relay-server-port}",
            "users": [
              {
                "user": "${relay-user}",
                "pass": "${relay-password}"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "allowInsecure": true
        }
      },
      "mux": {
        "enabled": true,
        "concurrency": 8
      }
    }
  ]
}